
{
  "name": "ai-code-annotator",
  "displayName": "AI Code Annotator",
  "description": "Detects Copilot generated code and adds annotations automatically.",
  "version": "0.0.1",
  "publisher": "kumarankur1729",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": ["*"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.handlePaste",
        "title": "Handle Paste"
      }
    ],
    "keybindings": [
      {
        "command": "extension.handlePaste",
        "key": "ctrl+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x",
    "typescript": "^5.0.0"
  }
}

